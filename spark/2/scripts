create 'statistics', 'f1' 
create 'settings', 'f1' 

("10.0.2.15", 1, 40, 30)
put 'settings', '10.0.2.15', 'f1:type', '1' 
put 'settings', '10.0.2.15', 'f1:value', '10000' 
put 'settings', '10.0.2.15', 'f1:period', '30' 
put 'settings', '10.0.2.15', 'f1:alerted', 'false' 


